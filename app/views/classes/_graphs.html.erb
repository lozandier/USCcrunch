<div class="span4">
  <div class="squiggly-border"></div>
  <div class="graph" id="flot-gradient-line_<%= @user.id %>"></div>
  <div class="stats_box">
    <div class="stats_box_time_row">
      <%= link_to "1d", graphs_class_path(@user.school_admin.school,@user, :point => '1d'), :remote => true  %>
      <%= link_to "5d", graphs_class_path(@user.school_admin.school,@user, :point => '5d'), :remote => true  %>
      <%= link_to "1m", graphs_class_path(@user.school_admin.school,@user, :point => '1m'), :remote => true  %>
      <%= link_to "2m", graphs_class_path(@user.school_admin.school,@user, :point => '2m'), :remote => true  %>
      <%= link_to "3m", graphs_class_path(@user.school_admin.school,@user, :point => '3m'), :remote => true  %>
      <%= link_to "All", graphs_class_path(@user.school_admin.school,@user, :point => 'all'), :remote => true  %><br />
      From: <%= text_field_tag :from, @datess %>
      To: <%= text_field_tag :to, @tt %>
    </div>
    <style>
      #from, #to{
        width: 75px;
        height: 12px;
        margin: 2px 0px;
      }
    </style>
    <div class="stats_box_row">
      Posts <span class="stats_num"><%= @posts %></span>
    </div>
    <div class="stats_box_row">
      Replies <span class="stats_num"><%= @replies %></span>
    </div>
    <div class="stats_box_row">
      Time Online <span class="stats_num"><%= @user.sign_in_count %>h</span>
    </div>
    <div class="stats_box_row">
      Favorites <span class="stats_num"><%= @favorites %></span>
    </div>
    <div class="stats_box_row">
      Favorited <span class="stats_num"><%= @user_favorites %></span>
    </div>
  </div>
</div>
<script type="text/javascript">
  jQuery(function($){
    flot_line('flot-gradient-line_<%= @user.id %>', flot_options_line,<%= @likesqw %>,<%= @likesqw1 %>);
  });
</script>
<style>
  .tickLabel{
    right: 0px;
  }
</style>