<div class="span4">
  <div class="squiggly-border"></div>
  <div class="graph" id="flot-gradient-line_<%= @user.id %>"></div>
  <div class="stats_box">
    <div class="stats_box_time_row">
      <%= link_to "1d", graphs_class_path(@user, :point => '1d'), :remote => true  %>
      <%= link_to "5d", graphs_class_path(@user, :point => '5d'), :remote => true  %>
      <%= link_to "1m", graphs_class_path(@user, :point => '1m'), :remote => true  %>
      <%= link_to "2m", graphs_class_path(@user, :point => '2m'), :remote => true  %>
      <%= link_to "3m", graphs_class_path(@user, :point => '3m'), :remote => true  %>
      <%= link_to "All", graphs_class_path(@user, :point => 'all'), :remote => true  %>
      <%= image_tag("/assets/tt.png") %>
    </div>
    <div class="stats_box_row">
      Posts <span class="stats_num"><%= @user.tweets.where('reply IS NULL and post_box IS NULL').count %></span>
    </div>
    <div class="stats_box_row">
      Replies <span class="stats_num"><%= @user.tweets.where('reply IS NOT NULL and post_box IS NULL').count %></span>
    </div>
    <div class="stats_box_row">
      Time Online <span class="stats_num">125h</span>
    </div>
    <div class="stats_box_row">
      Favorites <span class="stats_num">15</span>
    </div>
    <div class="stats_box_row">
      Favorited <span class="stats_num">15</span>
    </div>
  </div>
</div>
<script type="text/javascript">
  jQuery(function($){
    flot_line('flot-gradient-line_<%= @user.id %>', flot_options_line,[[<%= @time5 %>, <%= @likes5 %>],[<%= @time4 %>, <%= @likes4 %>],[<%= @time3 %>, <%= @likes3 %>],[<%= @time2 %>, <%= @likes2 %>],[<%= @time1 %>, <%= @likes1 %>],[<%= @time %>, <%= @likes %>]],[[<%= @time5 %>, <%= @clikes5 %>],[<%= @time4 %>, <%= @clikes4 %>],[<%= @time3 %>, <%= @clikes3 %>],[<%= @time2 %>, <%= @clikes2 %>],[<%= @time1 %>, <%= @clikes1 %>],[<%= @time %>, <%= @clikes %>]]);
  });
</script>
<style>
  .tickLabel{
    right: 0px;
}
</style>