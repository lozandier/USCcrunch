<div id="Tweet-summary" style="float: left;left: 210px;position: absolute;top: 65px;width: 70%;">
  <div id="title">Students</div>
  <% if @students.present? %>
    <table border="1" width="100%">
      <tr>
        <th>Student Name</th>
        <th>Email</th>
      </tr>
      <% for school in @students %>
        <tr style="text-align: center;">
          <td><%= school.email %></td>
          <td><%= link_to school.username, admin_student_path(school) %></td>
        </tr>
      <% end %>
    </table>
  <% else %>
    <span style="color: red;font-size: 15px;left: 166px;position: relative;top: 17px;">No Students.</span>
  <% end %>
</div>

